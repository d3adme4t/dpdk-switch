# buffer size (in total) in KiB
buffer_size = 5120
shared_memory=false
buffer_management_policy = "Equal Division"
dt_shift_alpha = 0
log_queue_length = true
log_queue_length_port = 0
queue_length_file = "/dev/shm/dpdk-qlen.log"
ecn_enable = true
# ecn threshold in KB
ecn_threshold = 200
# rate limit
tx_rate_mbps = 999
# bucket size in bytes
bucket_size = 5000
# cedm configuration
cedm_enable = false
cedm_ws = 0.129
# if k2=0 (or k2 < ecn_threshold), then automatically determine k2
cedm_k2_kb = 0
